{
  "version": 2,
  "builds": [{ "src": "server/server.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/server/server.js" }],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
